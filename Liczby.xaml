<Window x:Class="probna_kartkowka_2.Liczby"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:probna_kartkowka_2"
        mc:Ignorable="d"
        Title="Liczby" Height="137" Width="300">
    <StackPanel>
        <UniformGrid Columns="2" >
            <TextBlock Padding="5">Ile liczb</TextBlock>
            <TextBox x:Name="ile"></TextBox>
            <TextBlock Padding="5">Od ilu</TextBlock>
            <TextBox x:Name="od_ile"></TextBox>
            <TextBlock Padding="5">Do ilu</TextBlock>
            <TextBox x:Name="do_ile"></TextBox>
        </UniformGrid>
        <Button Click="losowanie">Zaaplikuj</Button>
    </StackPanel>
</Window>
